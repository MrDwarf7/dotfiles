[
  {
    "context": "VimControl && !menu",
    "bindings": {
      "i": ["vim::PushOperator", { "Object": { "around": false } }],
      "a": ["vim::PushOperator", { "Object": { "around": true } }],
      "h": "vim::Left",
      "left": "vim::Left",
      "backspace": "vim::Backspace",
      // Honestly not too sure what this solves lol
      "j": [
        "vim::Down",
        {
          "displayLines": true
        }
      ],
      // Honestly not too sure what this solves lol
      "down": [
        "vim::Down",
        {
          "displayLines": true
        }
      ],
